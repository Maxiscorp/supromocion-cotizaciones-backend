<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\ProductosPreciosUnitarios" table="productos_precios_unitarios">
    <indexes>
      <index name="IDX_7971566ED6E4E18" columns="idproducto"/>
    </indexes>
    <id name="idprecioProductoUnitario" type="integer" column="idprecio_producto_unitario">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="cantidad" type="integer" column="cantidad" nullable="true"/>
    <field name="precioUnitario" type="decimal" column="precio_unitario" precision="10" scale="2" nullable="true"/>
    <field name="fechaMod" type="datetime" column="fecha_mod" nullable="false"/>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <many-to-one field="idproducto" target-entity="Productos">
      <join-columns>
        <join-column name="idproducto" referenced-column-name="idproducto"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
