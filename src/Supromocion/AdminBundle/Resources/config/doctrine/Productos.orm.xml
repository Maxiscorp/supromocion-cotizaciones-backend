<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\Productos" table="productos">
    <indexes>
      <index name="IDX_767490E6D4B68681" columns="idmoneda"/>
      <index name="IDX_767490E6F1B40676" columns="idproducto_tipo"/>
    </indexes>
    <id name="idproducto" type="integer" column="idproducto">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="string" column="nombre" nullable="true"/>
    <field name="descripcion" type="string" column="descripcion" nullable="true"/>
    <field name="codigoProductoInterno" type="string" column="codigo_producto_interno" nullable="true"/>
    <field name="codigoProductoProveedor" type="string" column="codigo_producto_proveedor" nullable="true"/>
    <field name="fechaAlta" type="datetime" column="fecha_alta" nullable="false"/>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <many-to-one field="idproductoTipo" target-entity="ProductosTipos">
      <join-columns>
        <join-column name="idproducto_tipo" referenced-column-name="idproducto_tipo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idmoneda" target-entity="Monedas">
      <join-columns>
        <join-column name="idmoneda" referenced-column-name="idmoneda"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
