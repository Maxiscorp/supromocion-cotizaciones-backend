<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\OrdenesItems" table="ordenes_items">
    <indexes>
      <index name="IDX_328BE621338E2456" columns="idcotizacion_item"/>
      <index name="IDX_328BE621372DA3CE" columns="idorden"/>
    </indexes>
    <id name="idordenItem" type="integer" column="idorden_item">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
    <field name="activo" type="boolean" column="activo" nullable="true"/>
    <many-to-one field="idorden" target-entity="Ordenes">
      <join-columns>
        <join-column name="idorden" referenced-column-name="idorden"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idcotizacionItem" target-entity="CotizacionesParcialesDetalle">
      <join-columns>
        <join-column name="idcotizacion_item" referenced-column-name="idcotizacion_parcial_detalle"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
