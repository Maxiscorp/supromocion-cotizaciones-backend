<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\ImpresionesTiposColoresPreciosUnitarios" table="impresiones_tipos_colores_precios_unitarios">
    <indexes>
      <index name="IDX_C4DF8C38BCFD0BD0" columns="idimpresion_tipo"/>
      <index name="IDX_C4DF8C388F19E18" columns="idimpresion_color"/>
    </indexes>
    <id name="idimpresionTipoColorPrecioUnitario" type="integer" column="idimpresion_tipo_color_precio_unitario">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="cantidad" type="integer" column="cantidad" nullable="false"/>
    <field name="precioUnitario" type="decimal" column="precio_unitario" precision="8" scale="2" nullable="true"/>
    <field name="fechaIngreso" type="datetime" column="fecha_ingreso" nullable="false"/>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <many-to-one field="idimpresionColor" target-entity="ImpresionesColores">
      <join-columns>
        <join-column name="idimpresion_color" referenced-column-name="idimpresion_colores"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idimpresionTipo" target-entity="ImpresionesTipos">
      <join-columns>
        <join-column name="idimpresion_tipo" referenced-column-name="idimpresion_tipo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
