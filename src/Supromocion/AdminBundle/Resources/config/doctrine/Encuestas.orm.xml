<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\Encuestas" table="encuestas">
    <indexes>
      <index name="IDX_1A67E5657989D9F9" columns="idencuesta_tipo"/>
    </indexes>
    <id name="idencuesta" type="integer" column="idencuesta">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="string" column="nombre" nullable="true"/>
    <field name="fechaInicio" type="datetime" column="fecha_inicio" nullable="false"/>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <many-to-one field="idencuestaTipo" target-entity="EncuestasTipos">
      <join-columns>
        <join-column name="idencuesta_tipo" referenced-column-name="idencuesta_tipo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
