<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\ProductosImpresionesTipos" table="productos_impresiones_tipos">
    <indexes>
      <index name="IDX_BDD44F59D6E4E18" columns="idproducto"/>
      <index name="IDX_BDD44F59285BC26C" columns="idmpresion_tipo"/>
    </indexes>
    <id name="idproductoImpresionTipo" type="integer" column="idproducto_impresion_tipo">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <many-to-one field="idmpresionTipo" target-entity="ImpresionesTipos">
      <join-columns>
        <join-column name="idmpresion_tipo" referenced-column-name="idimpresion_tipo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idproducto" target-entity="Productos">
      <join-columns>
        <join-column name="idproducto" referenced-column-name="idproducto"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
