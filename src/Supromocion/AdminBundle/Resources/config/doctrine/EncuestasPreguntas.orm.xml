<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\EncuestasPreguntas" table="encuestas_preguntas">
    <indexes>
      <index name="IDX_2B0A88FA188E204C" columns="idencuesta"/>
      <index name="IDX_2B0A88FACA1872FC" columns="idencuesta_pregunta_tipo"/>
    </indexes>
    <id name="idencuestaPregunta" type="integer" column="idencuesta_pregunta">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" nullable="true"/>
    <field name="fechaIngreso" type="datetime" column="fecha_ingreso" nullable="false"/>
    <field name="orden" type="integer" column="orden" nullable="false"/>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <many-to-one field="idencuestaPreguntaTipo" target-entity="EncuestasPreguntasTipos">
      <join-columns>
        <join-column name="idencuesta_pregunta_tipo" referenced-column-name="idencuesta_pregunta_tipo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idencuesta" target-entity="Encuestas">
      <join-columns>
        <join-column name="idencuesta" referenced-column-name="idencuesta"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
