<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\Operadores" table="operadores">
    <indexes>
      <index name="IDX_9A9572FE679F8138" columns="idoperador_tipo"/>
    </indexes>
    <id name="idoperador" type="integer" column="idoperador">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="string" column="nombre" nullable="true"/>
    <field name="apellido" type="string" column="apellido" nullable="true"/>
    <field name="usuario" type="string" column="usuario" nullable="true"/>
    <field name="password" type="text" column="password" nullable="true"/>
    <field name="email" type="string" column="email" nullable="true"/>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <many-to-one field="idoperadorTipo" target-entity="OperadoresTipos">
      <join-columns>
        <join-column name="idoperador_tipo" referenced-column-name="idoperador_tipo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
