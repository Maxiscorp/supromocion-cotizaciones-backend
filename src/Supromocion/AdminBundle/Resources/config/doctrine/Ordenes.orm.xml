<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\Ordenes" table="ordenes">
    <indexes>
      <index name="IDX_55FDC04A2B18C94B" columns="idcliente"/>
      <index name="IDX_55FDC04A890CF139" columns="idcotizacion"/>
    </indexes>
    <id name="idorden" type="integer" column="idorden">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nroPresupuesto" type="string" column="nro_presupuesto" nullable="true"/>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
    <field name="lugarEntrega" type="string" column="lugar_entrega" nullable="true"/>
    <field name="activo" type="boolean" column="activo" nullable="true"/>
    <many-to-one field="idcotizacion" target-entity="Cotizaciones">
      <join-columns>
        <join-column name="idcotizacion" referenced-column-name="idcotizacion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idcliente" target-entity="Clientes">
      <join-columns>
        <join-column name="idcliente" referenced-column-name="idcliente"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
