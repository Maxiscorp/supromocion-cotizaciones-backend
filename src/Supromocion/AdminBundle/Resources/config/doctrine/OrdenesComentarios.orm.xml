<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\OrdenesComentarios" table="ordenes_comentarios">
    <indexes>
      <index name="IDX_D7CFDCF372DA3CE" columns="idorden"/>
    </indexes>
    <id name="idordenComentario" type="integer" column="idorden_comentario">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="comentario" type="string" column="comentario" nullable="true"/>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
    <field name="activo" type="boolean" column="activo" nullable="true"/>
    <many-to-one field="idorden" target-entity="Ordenes">
      <join-columns>
        <join-column name="idorden" referenced-column-name="idorden"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
