<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Supromocion\AdminBundle\Entity\EncuestasResultados" table="encuestas_resultados">
    <indexes>
      <index name="IDX_497E7024765C278B" columns="idencuesta_pregunta"/>
      <index name="IDX_497E70242B18C94B" columns="idcliente"/>
    </indexes>
    <id name="idencuestaRespuesta" type="integer" column="idencuesta_respuesta">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="idasociado" type="integer" column="idasociado" nullable="true"/>
    <field name="valorNumerico" type="integer" column="valor_numerico" nullable="true"/>
    <field name="valorTexto" type="string" column="valor_texto" nullable="true"/>
    <field name="fechaIngreso" type="datetime" column="fecha_ingreso" nullable="false"/>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <many-to-one field="idcliente" target-entity="Clientes">
      <join-columns>
        <join-column name="idcliente" referenced-column-name="idcliente"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idencuestaPregunta" target-entity="EncuestasPreguntas">
      <join-columns>
        <join-column name="idencuesta_pregunta" referenced-column-name="idencuesta_pregunta"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
